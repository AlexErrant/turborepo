{
  "name": "ui",
  "version": "0.0.0",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.mjs"
      }
    },
    "./client": {
      "import": {
        "types": "./dist/client/index.d.ts",
        "default": "./dist/client/index.mjs"
      }
    },
    "./styles.css": "./dist/index.css"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/",
    "dev": "watch 'tsup --watch' src/",
    "check-types": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.5",
    "autoprefixer": "^10.4.13",
    "eslint-config-custom": "workspace:*",
    "fs-extra": "^11.1.1",
    "postcss": "^8.4.20",
    "react": "^18.2.0",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.2.4",
    "tsconfig": "workspace:*",
    "tsup": "^6.1.3",
    "typescript": "^5.1.6",
    "watch": "^1.0.2"
  },
  "files": [
    "dist"
  ]
}
